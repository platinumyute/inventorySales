@model IEnumerable<InventorySales.Models.ProductBrand>
<div class="panel panel-default">
     <div class="panel-heading">
          <b>Recent Brand Entries</b>
     </div>
     <div class="panel-body">
          <ul class="list-group">
               @foreach (var item in Model)
               {
                    <li class="list-group-item">
                         <div>
                              Name: @item.Name
                         </div>
                    </li>
               }
          </ul>
     </div>
</div>
